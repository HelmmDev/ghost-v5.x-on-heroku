<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "BreadcrumbList",
  "itemListElement": [
    {
      "@type": "ListItem",
      "position": 1,
      "name": "Home",
      "item": "https://www.helmm.com/lifeathelmm"
    }
    {{#if primary_tag}}
    ,{
      "@type": "ListItem",
      "position": 2,
      "name": "{{primary_tag.name}}",
      "item": "https://www.helmm.com/lifeathelmm/{{primary_tag.slug}}"
    }
    ,{
      "@type": "ListItem",
      "position": 3,
      "name": "{{title}}",
      "item": "https://www.helmm.com/lifeathelmm{{url}}"
    }
    {{else if title}}
    ,{
      "@type": "ListItem",
      "position": 2,
      "name": "{{title}}",
      "item": "https://www.helmm.com/lifeathelmm{{url}}"
    }
    {{/if}}
  ]
}
</script>
